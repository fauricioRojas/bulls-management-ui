{
  "name": "bulls-management-ui",
  "version": "0.1.0",
  "private": true,
  "author": "Fauricio Rojas Hernandez",
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "classnames": "2.2.6",
    "font-awesome": "4.7.0",
    "formik": "2.1.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-jss": "10.3.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "3.7.2",
    "yup": "0.29.1"
  },
  "devDependencies": {
    "@types/classnames": "2.2.10",
    "@types/jest": "24.0.0",
    "@types/node": "12.0.0",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "@types/react-jss": "10.0.0",
    "@types/react-router-dom": "5.1.5",
    "@types/yup": "0.29.3",
    "env-cmd": "10.1.0",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "5.0.0"
  },
  "scripts": {
    "start": "env-cmd -f env/.env npm run start-app",
    "start-app": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --no-cache --watchAll=true --modulePaths=src",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/modules/**/*.(ts|tsx)",
      "src/shared/components/**/*.(ts|tsx)",
      "src/shared/context/**/*.tsx",
      "src/shared/hooks/**/*.(ts|tsx)",
      "!src/**/*.style.ts",
      "!src/shared/routes/*.(ts|tsx)",
      "!src/shared/style/**/*.ts",
      "!src/shared/services/**/*.ts",
      "!src/**/*.interface.ts",
      "!src/**/*.constants.ts",
      "!src/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 90
      }
    }
  }
}